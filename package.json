{
  "name": "tsconfig",
  "version": "1.0.0",
  "description": "Configuração do TypeScript",
  "main": "index.js",
  "author": {
    "name": "Kaique Araújo",
    "email": "gottium@gmail.com"
  },
  "keywords": [
    "TypeScript"
  ],
  "license": "MIT",
  "scripts": {
    "start": "node dist/server.js",
    "build": "tsc && tscpaths --project tsconfig.json -s ./src -o ./dist",
    "dev": "tsnd -r tsconfig-paths/register --files src/server.ts --ignore-watch node_modules"
  },
  "private": true,
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.1.2",
    "ts-node-dev": "^1.0.0-pre.62",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "tscpaths": "^0.0.9"
  }
}
